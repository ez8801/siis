extends layout

block content
  script(type='text/javascript').
    $(document).ready(function(){
      $("#dataroom").addClass('active');
    });
  ul.collapsible.popout(data-collapsible='accordion')
    each obj, i in res
      li
        .collapsible-header
          i.material-icons filter_drama
          | #{obj.Folder}
        .collapsible-body
          div.collection
            each file in obj.Files
              div.collection-item
                a(href='/download?where=' + "#{file.Idx}", target='_self') #{file.FileName}